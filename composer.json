{
    "name": "dymantic/slideshow",
    "description": "Simple package for managing user created slideshows. Does not include frontend or view stuff.",
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Joyner",
            "email": "joyner.michael@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "^5.7",
        "illuminate/database": "^5.7",
        "illuminate/queue": "^5.7",
        "spatie/laravel-medialibrary": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "orchestra/testbench": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "Dymantic\\Slideshow\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dymantic\\Slideshow\\Tests\\": "tests"
        }
    }
}
